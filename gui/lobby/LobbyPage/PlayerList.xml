<?xml version="1.0" encoding="utf-8" ?>
<object>
  <object
    name="playersFilter"
    type="input"
    style="ModernInput"
    size="0 -6 100% 16"
    font="sans-bold-13"
    placeholder_text="Search for Player"
  />
  <object
    name="playersBox"
    style="ModernSortedList"
    size="0 15 100% 100%"
    selected_column="name"
    selected_column_order="1"
    type="olist"
    sortable="true"
    font="sans-bold-14">
    <column id="buddy" width="14" />
    <column id="status" width="16">
      <translatableAttribute id="heading" />
    </column>
    <column id="name" width="60%-32">
      <translatableAttribute id="heading">Name</translatableAttribute>
    </column>
    <column id="rating" width="30%">
      <translatableAttribute id="heading">Rating</translatableAttribute>
    </column>
  </object>
</object>
